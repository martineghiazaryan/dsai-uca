crs:Math101 rdf:type cls:Course ;
         prp:courseTitle "Calculus I" ;
         prp:courseDescription "An introductory course in calculus." ;
         prp:partOfProgram crs:ComputerScience ;
         prp:taughtBy ind:Ava .


crs:Psychology101 rdf:type cls:Course ;
               prp:courseTitle "Introduction to Psychology" ;
               prp:courseDescription "A foundational course in psychology." ;
               prp:partOfProgram crs:BachelorPsychology ;
               prp:taughtBy ind:DrJohnson .


crs:Physics101 rdf:type cls:Course ;
            prp:courseTitle "Introduction to Physics" ;
            prp:courseDescription "A basic course in Physics for beginners." ;
            prp:partOfProgram crs:BachelorPhysics ;
            prp:taughtBy ind:DrSmith .



crs:Chemistry101 rdf:type cls:Course ;
              prp:courseTitle "Introduction to Chemistry" ;
              prp:courseDescription "A basic course in Chemistry for beginners." ;
              prp:partOfProgram crs:BachelorChemistry ;
              prp:taughtBy ind:Olivia

crs:Literature101 rdf:type cls:Course ;
              prp:courseTitle "Introduction to Literature" ;
              prp:courseDescription "A foundational course in literature." ;
              prp:partOfProgram crs:BachelorLiterature ;
              prp:taughtBy ind:Isabella .

crs:History101 rdf:type cls:Course ;
            prp:courseTitle "Introduction to History" ;
            prp:courseDescription "A foundational course in history, covering major historical events and periods." ;
            prp:partOfProgram crs:BachelorHistory ;
            prp:taughtBy ind:Harper .

crs:Math102 rdf:type cls:Course ;
                prp:courseTitle "Calculus II" ;
                prp:courseDescription "A continuation of Calculus I, covering more advanced topics." ;
                prp:partOfProgram crs:ComputerScience ;
                prp:taughtBy ind:Ava .

crs:ComputerProgramming rdf:type cls:Course ;
                prp:courseTitle "Introduction to Computer Programming" ;
                prp:courseDescription "An introduction to computer programming using Python." ;
                prp:partOfProgram crs:ComputerScience ;
                prp:taughtBy ind:Elijah .

crs:Physics102 rdf:type cls:Course ;
                prp:courseTitle "Physics II" ;
                prp:courseDescription "A continuation of Physics I, covering more advanced topics." ;
                prp:partOfProgram crs:BachelorPhysics ;
                prp:taughtBy ind:DrSmith .

crs:Chemistry102 rdf:type cls:Course ;
                prp:courseTitle "Chemistry II" ;
                prp:courseDescription "A continuation of Chemistry I, covering more advanced topics." ;
                prp:partOfProgram crs:BachelorChemistry ;
                prp:taughtBy ind:Olivia .

crs:WorldLiterature rdf:type cls:Course ;
                prp:courseTitle "World Literature" ;
                prp:courseDescription "A study of literature from different cultures and time periods." ;
                prp:partOfProgram crs:BachelorLiterature ;
                prp:taughtBy ind:Isabella .

crs:ModernHistory rdf:type cls:Course ;
                prp:courseTitle "Modern History" ;
                prp:courseDescription "A study of history from the 18th century to the present." ;
                prp:partOfProgram crs:BachelorHistory ;
                prp:taughtBy ind:Harper .


